{
  "name": "fracttal-react",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "serve": "vite build &&vite preview --port 5002 --strictPort",
    "start": "vite --host",
    "preview": "vite preview",
    "preview:micro": "cross-env NODE_OPTIONS=--max-old-space-size=18192 vite build --mode production && vite preview",
    "web:build": "cross-env NODE_OPTIONS=--max-old-space-size=18192 vite build --mode production",
    "npm:update": "npm update --force --save",
    "npm:install": "npm install --force --save",
    "reinstall": "npx rimraf node_modules && npx rimraf package-lock.json && npm cache clean --force && npm install --force --save",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
