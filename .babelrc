{
  "plugins": ["@babel/plugin-syntax-optional-chaining"],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-syntax-optional-chaining",
        ["istanbul", {
          "exclude": [
            "**/*.spec.{js,jsx,ts,tsx}",
            "**/*.test.{js,jsx,ts,tsx}",
            "**/*.d.ts",
            "**/node_modules/**",
            "**/test/**"
          ]
        }]
      ]
    }
  }
}